{
    "plugins": [
      "security"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:security/recommended"
    ],
    "rules": {
      "no-console": "warn",
      "no-unused-vars": "warn",
      "no-undef": "warn",
      "security/detect-object-injection": "warn",
      "security/detect-non-literal-require": "warn",
      "plugins": ["react"],
      "settings": {
        "react": {
          "version": "detect"
        }
      }
    }
  }
  